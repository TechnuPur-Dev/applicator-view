{
  "version": 2,
  "builds": [
    {
      "src": "apps/user-service/index.ts",
      "use": "@vercel/node"
    },
    {
      "src": "apps/job-service/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/user-service/(.*)",
      "dest": "apps/user-service/index.ts"
    },
    {
      "src": "/api/job-service/(.*)",
      "dest": "apps/job-service/index.ts"
    }
  ]
}

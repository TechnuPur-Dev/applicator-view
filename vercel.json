{
  "version": 2,
  "builds": [
    {
      "src": "apps/user-service/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["apps/user-service/**", "node_modules/**", "tsconfig.json", "package.json"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/user-service/index.ts"
    }
  ]
}

{
  "version": 2,
  "builds": [
    {
      "src": "apps/user-service/index.ts",
      "use": "@vercel/node"
    }
  ],
  "functions": {
    "apps/user-service/index.ts": {
      "runtime": "nodejs",
      "includeFiles": [
        "apps/user-service/**",
        "node_modules/**",
        "package.json",
        "tsconfig.json",
        "tsconfig.build.json"
      ]
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/user-service/index.ts"
    }
  ]
}
